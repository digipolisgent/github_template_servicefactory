<?xml version="1.0"?> 
<settings>
    <mirrors>
        <mirror>
            <id>digipolis_nexus</id>
            <name>Maven Repository Manager at Digipolis</name>
            <url>http://nexus-release-management.servicesqa.local/repository/maven-public/</url>
            <mirrorOf>*</mirrorOf>
        </mirror>
    </mirrors>
    <servers>
        <server>
            <id>digipolis_nexus</id>
            <username>${env.NEXUS_USER}</username>
            <password>${env.NEXUS_PASSWORD}</password>
        </server>
    </servers>
</settings>
